{
  "name": "doujin-meta",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "A metadata database for doujin music.",
  "keywords": [
    "metadata",
    "music",
    "database"
  ],
  "author": "Grant Howard",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.27.2",
    "fuse.js": "^6.6.2",
    "primeicons": "^6.0.1",
    "primevue": "^3.17.0",
    "touhou-tagger": "1.7.0-beta.0",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@the1812/eslint-config": "^1.0.0",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vercel/node": "^2.9.3",
    "@vitejs/plugin-vue": "^4.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.10.0",
    "postcss": "^8.4.21",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.3",
    "vercel": "^28.15.4",
    "vite": "^4.2.1",
    "vue-tsc": "^1.2.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "primeicons": "6"
      }
    },
    "patchedDependencies": {
      "primeicons@6.0.1": "patches/primeicons@6.0.1.patch",
      "@vercel/node@2.9.3": "patches/@vercel__node@2.9.3.patch",
      "primevue@3.17.0": "patches/primevue@3.17.0.patch"
    }
  },
  "scripts": {
    "vercel:dev": "vercel dev",
    "vercel:deploy": "vercel",
    "dev": "vite --port 3000",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "eslint-check": "eslint . --ext .cjs,.ts,.vue",
    "eslint-fix": "eslint . --ext .cjs,.ts,.vue --fix",
    "lint": "vue-tsc --noEmit && eslint . --ext .cjs,.ts,.vue"
  }
}
