{
  "name": "doujin-meta",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "A metadata database for doujin music.",
  "keywords": [
    "metadata",
    "music",
    "database"
  ],
  "author": "Grant Howard",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "fuse.js": "^6.6.2",
    "primeicons": "^6.0.1",
    "primevue": "^3.17.0",
    "touhou-tagger": "^1.11.3",
    "vue": "^3.4.18",
    "vue-i18n": "^9.9.1",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@the1812/eslint-config": "^1.2.1",
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vercel/node": "^3.1.1",
    "@vitejs/plugin-vue": "^4.6.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "postcss": "^8.4.35",
    "prettier": "3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.0.3",
    "vercel": "^34.1.11",
    "vite": "^4.5.2",
    "vue-tsc": "^1.8.27"
  },
  "pnpm": {
    "overrides": {
      "caniuse-lite": "1.0.30001585"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "primeicons": "6"
      }
    },
    "patchedDependencies": {
      "primeicons@6.0.1": "patches/primeicons@6.0.1.patch",
      "primevue@3.17.0": "patches/primevue@3.17.0.patch"
    }
  },
  "scripts": {
    "vercel:dev": "vercel dev",
    "vercel:deploy": "vercel",
    "dev": "vite --port 3000",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "eslint-check": "eslint . --ext .cjs,.ts,.vue",
    "eslint-fix": "eslint . --ext .cjs,.ts,.vue --fix",
    "lint": "vue-tsc --noEmit && eslint . --ext .cjs,.ts,.vue"
  }
}
